    "rxjs": "^6.6.6"
  },
  "devDependencies": {
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "inquirer-directory": "^2.2.0",
    "node-sass": "^5.0.0",
    "plop": "^2.7.4",
    "typed-scss-modules": "^4.1.1",
    "typescript": "^4.2.3"
